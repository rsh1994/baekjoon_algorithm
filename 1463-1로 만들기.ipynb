   "source": [
    "n = int(input())\n",
    "\n",
    "arr = [ 0 for _ in range(n+1)]\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    arr[i] = arr[i-1] + 1\n",
    "    \n",
    "    if i%2==0 and arr[i] > arr[i//2] + 1:\n",
    "        arr[i] = arr[i//2] + 1\n",
    "        \n",
    "    if i%3==0 and arr[i] > arr[i//3] + 1:\n",
    "        arr[i] = arr[i//3] + 1\n",
    "        \n",
    "print(arr[-1])\n",
    "\n",
    "\n"
   ]
