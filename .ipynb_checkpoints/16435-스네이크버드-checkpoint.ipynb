    "arr = list(map(int,input().split()))\n",
    "arr.sort()\n",
    "\n",
    "while True:\n",
    "    if len(arr)==0:\n",
    "        break\n",
    "    if arr[0] <= l:\n",
    "        arr.pop(0)\n",
    "        l += 1\n",
    "    else:\n",
    "        break\n",
    "    \n",
