    "n = int(input())\n",
    "\n",
    "room = []\n",
    "cnt = 0\n",
    "end_time = 0\n",
    "for i in range(n):\n",
    "    room.append(list(map(int,input().split())))\n",
    "    \n",
    "\n",
    "room.sort(key=lambda x: (x[1],x[0]))\n",
    "\n",
    "for a,b in room:\n",
    "    if a>=end_time:\n",
    "        cnt+=1\n",
    "        end_time = b\n",
    "print(cnt)\n",
    "    "
