   "source": [
    "n, m = map(int, input().split())\n",
    "\n",
    "arr = [i for i in range(1,n+1)]\n",
    "result = []\n",
    "idx = m-1\n",
    "while True:\n",
    "    result.append(str(arr.pop(idx)))\n",
    "    if arr==[]:\n",
    "        break\n",
    "    idx = (idx+m-1)%len(arr)\n",
    "    \n",
    "print(\"<\", end='')\n",
    "print(', '.join(result), end='')\n",
    "print(\">\")\n",
    "    "
   ]
