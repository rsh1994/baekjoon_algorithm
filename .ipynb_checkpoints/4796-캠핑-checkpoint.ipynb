   "source": [
    "idx = 1\n",
    "while True:\n",
    "    a,b,c = map(int,input().split())\n",
    "    if a==b==c==0:\n",
    "        break\n",
    "    d = c//b\n",
    "    q = c-d*b\n",
    "    \n",
    "    answer = a*d\n",
    "    answer += min(a,q)\n",
    "    print(\"Case {}: {}\".format(idx, answer))\n",
    "    idx += 1"
   ]
