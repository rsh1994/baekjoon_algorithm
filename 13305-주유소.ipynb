    "n = int(input())\n",
    "m = list(map(int,input().split()))\n",
    "p = list(map(int,input().split()))\n",
    "\n",
    "min_price = p[0]\n",
    "\n",
    "answer = 0\n",
    "\n",
    "for i in range(len(m)):\n",
    "    if min_price > p[i]:\n",
    "        min_price = p[i]\n",
    "    answer += min_price * m[i]\n",
    "    \n",
    "print(answer)\n",
    "    "
